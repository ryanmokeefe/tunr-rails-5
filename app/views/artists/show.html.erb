<h1> <%= @artist.name %> <a href="/artists/<%= @artist.id%>/edit">(edit)</a></h1>

<img class='artist-photo' src="<%= @artist.photo_url %>">

<h3>Songs</h3>
<ul>
    <% @artist.songs.each do |song| %>
        <li>
            <a href="/songs/<%= song.id %>">
            <%= song.title %> (<%= song.album %>) 
            </a>
        </li>
    <% end %>

</ul>
